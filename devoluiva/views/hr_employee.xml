<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_employee_form" model="ir.ui.view">
        <field name="name">hr.employee.form.devoluiuva</field>
        <field name="model">hr.employee</field>
        <field ref="hr.view_employee_form" name="inherit_id"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[1]" position="inside">
                <page string="Devoluiva">
                    <group>
                        <field name="devoluiva_id_customer"/>
                        <field name="devoluiva_journal_id"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>

